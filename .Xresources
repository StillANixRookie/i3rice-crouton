!!ROFI
rofi.font:              Ubuntu Mono 17 
rofi.location:          0
!rofi.fullscreen:        true
rofi.padding:           50
rofi.hide-scrollbar:    true
rofi.line-margin:       10
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             1000
rofi.lines:             5
!rofi.terminal:          i3-sensible-terminal
rofi.show-icons:	true
rofi.drun-icon-theme:	'Numix-Reborn-Circle'

!!Xft
Xft.dpi:		96
Xft.antialias:		true
Xft.hinting:		true
Xft.rgba:		rgb
Xft.autohint:		false
Xft.hintstyle:		hintslight
Xft.lcdfilter:		lcddefault

!!XTerm
XTerm*background:	blackd
XTerm*foreground:	whited
XTerm*pointerColor:	cyand
XTerm*faceName:		Fixed
XTerm*faceSize:		11
XTerm*reverseVideo:	on
XTerm*faceName:		Terminus:size=11:antialias=true
XTerm*selectToClipboard:true

Xcursor.theme:			xcursor-breeze
Xcursor.size:			0

!!urxvt
URxvt*font:			xft:Ubuntu Mono:size=18:antialias=true:hinting=true
URxvt*imFont:			xft:Ubuntu Mono:size=18:antialias=true:hinting=true
URxvt*boldFont:			xft:Ubuntu Mono:bold:size=18:antialias=true:hinting=true
URxvt*italicFont:		xft:Ubuntu Mono:italic:size=18:antialias=true:hinting=true
URxvt*boldItalicFont:		xft:Ubuntu Mono:bold:italic:size=18:antialias=true:hinting=true

URxvt*depth:			32
URxvt*scrollBar:		false
URxvt*mouseWheelScrollPage:	false
URxvt*cursorBlink:		true
URxvt*background:		rgba:0000/0000/0000/cc90
URxvt*foreground:		whitel
URxvt*saveLines:		5000
URxvt*useColor:			true
URxvt*internalBorder:		80

